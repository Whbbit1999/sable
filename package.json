    "artplayer": "^5.0.9",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "dayjs": "^1.11.8",
    "echarts": "^5.4.2",
    "lodash": "^4.17.21",
    "md-editor-v3": "^4.2.0",
    "pinia": "^2.1.4",
    "qrcode": "^1.5.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
